{
  "name": "print-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint 'src/**/*.ts'",
    "test": "jest",
    "cli": "ts-node src/cli.ts",
    "prisma:migrate": "pnpm --filter api run prisma:migrate",
    "prisma:seed": "pnpm --filter api run prisma:seed"
  },
  "dependencies": {
    "@spec/config": "workspace:*",
    "@repo/sdk": "workspace:*",
    "bullmq": "^4.12.2",
    "express": "^4.18.3",
    "escpos": "^4.0.0-alpha.6",
    "escpos-network": "^3.0.3",
    "escpos-usb": "^3.0.5",
    "pino": "^8.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
